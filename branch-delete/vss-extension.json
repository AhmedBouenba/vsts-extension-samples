{
    "manifestVersion": 1,
    "id": "samples-branch-delete",
    "version": "0.1.0",
    "name": "Branch Delete",
    "description": "Clean up old or unneeded Git branches right from within Visual Studio Online.",
    "baseUri": "https://localhost",
    "publisher": "fabrikam",
    "icons": {
        "default": "images/fabrikam-logo.png"
    },
    "scopes": ["vso.code-manage"]
    "tags": [
        "Sample"
    ],
    "categories": [
        "Productivity"
    ],
    "contributions": [
        {
            "id": "deleteBranchMenu",
            "type": "ms.vss-web.action",
            "description": "Delete action on the Git branches tree's context menu",
            "targets": [
                "ms.vss-code-web.git-branches-tree-menu"
            ],
            "properties": {
                "title": "Delete...",
                "icon": "images/delete.png",
                "group": "actions",
                "uri": "main.html"
            }
        }
    ]
}
