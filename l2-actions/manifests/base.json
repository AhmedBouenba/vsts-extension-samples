{
    "manifestVersion": 1,
    "id": "l2-actions-sample",
    "version": "0.1.8",
    "publisher": "ms-samples",
    "name": "Header L2 Actions Sample",
    "description": "Sample actions for header l2 section",
    "public": false,
    "tags": [
        "samples",
        "header",
        "action"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "categories": [
        "Developer samples"
    ],
    "contributions": [
        {
            "id": "l2-search-engines",
            "type": "ms.vss-tfs-web.hub-actions-menu",
            "targets": [
                "ms.vss-code-web.code-hub-group"
            ],
            "properties": {
                "order": 20,
                "uri": "index.html",
                "registeredObjectId": "l2-search-engines"
            }
        },
        {
            "id": "l2-dialog-opener",
            "type": "ms.vss-tfs-web.hub-actions-menu",
            "targets": [
                "ms.vss-code-web.files-hub-git"
            ],
            "properties": {
                "order": 10,
                "uri": "index.html",
                "registeredObjectId": "l2-dialog-opener"
            }
        },
        {
            "id": "dialog-content",
            "type": "ms.vss-web.control",
            "description": "The content displayed in the dialog.",
            "targets": [ ],
            "properties": {
                "uri": "dialog.html"
            }
        }       
    ]
}