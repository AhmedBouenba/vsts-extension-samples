{
    "manifestVersion": 1,
    "id": "samples-ui",
    "version": "0.1.0",
    "name": "UI Controls (Sample)",
    "description": "Explore different UI controls, including menus, toolbars, custom controls, and more.",
    "baseUri": "https://localhost",
    "publisher": "YOUR PUBLISHER HERE",
    "icons": {
        "wide": "images/fabrikam-logo.png",
        "default": "images/fabrikam-logo.png"
    },
    "tags": [
        "Sample"
    ],
    "categories": [
        "Sync and integration"
    ],
    "contributions": [
        {
            "id": "sample-ui-group",
            "targets": [
                "ms.vss-web.project-hub-groups-collection"
            ],
            "type": "ms.vss-web.hub-group",
            "properties": {
                "name": "Sample UI",
                "order": 100
            }
        },
        {
            "id": "simple-menu",
            "targets": [
                ".sample-ui-group"
            ],
            "type": "ms.vss-web.hub",
            "properties": {
                "name": "Simple",
                "order": 100,
                "uri": "menu.html"
            }
        },
        {
            "id": "control-menu",
            "targets": [
                ".sample-ui-group"
            ],
            "type": "ms.vss-web.hub",
            "properties": {
                "name": "Custom Control",
                "order": 100,
                "uri": "menu-control.html"
            }
        },
        {
            "id": "dropdown-menu",
            "targets": [
                ".sample-ui-group"
            ],
            "type": "ms.vss-web.hub",
            "properties": {
                "name": "Dropdown",
                "order": 100,
                "uri": "menu-dropdown.html"
            }
        },
        {
            "id": "context-menu",
            "targets": [
                ".sample-ui-group"
            ],
            "type": "ms.vss-web.hub",
            "properties": {
                "name": "Context Menu",
                "order": 100,
                "uri": "menu-context.html"
            }
        },
        {
            "id": "compareMenu",
            "targets": [
                "ms.vss-code-web.source-item-menu"
            ],
            "type": "ms.vss-web.action",
            "properties": {
                "title": "Compare...",
                "icon": "images/show-properties.png",
                "group": "actions",
                "uri": "menu-contributions.html"
            }
        },
        {
            "id": "compareToForm",
            "targets": [ ],
            "type": "ms.vss-web.control",
            "properties": {
                "uri": "compareToForm.html"
            }
        }
    ]
}