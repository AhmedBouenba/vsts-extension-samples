{
    "manifestVersion": 1,
    "extensionId": "samples-branch-delete",
    "version": "0.1.0",
    "name": "Branch Delete (Sample)",
    "description": "Clean up old or unneeded Git branches right from within Visual Studio Online.",
    "baseUri": "https://localhost",
    "publisher": "YOUR PUBLISHER HERE",
    "assets": [
        {
            "type": "Microsoft.VSO.LargeIcon",
            "path": "images/fabrikam-logo.png"
        }
    ],
    "tags": [
        "Sample"
    ],
    "categories": [
        "Productivity"
    ],
    "contributions": [
        {
            "id": "deleteBranchMenu",
            "targets": [
                "ms.vss-code-web.git-branches-tree-menu"
            ],
            "type": "ms.vss-web.action",
            "properties": {
                "title": "Delete...",
                "icon": "images/delete.png",
                "group": "actions",
                "uri": "main.html"
            }
        }
    ]
}