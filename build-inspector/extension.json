{
    "manifestVersion": 1,
    "extensionId": "samples-build-inspector",
    "version": "0.1.0",
    "name": "Build Inspector (Sample)",
    "description": "A simple extension that demonstrates how to show details for a Visual Studio Online resource.",
    "baseUri": "https://localhost",
    "publisher": "YOUR PUBLISHER HERE",
    "assets": [
        {
            "type": "Microsoft.VSO.LargeIcon",
            "path": "images/fabrikam-logo.png"
        }
    ],
    "tags": [
        "Sample"
    ],
    "categories": [
        "Build and release"
    ],
    "contributions": [
        {
            "id": "showCommits",
            "targets": [
                "ms.vss-build-web.completed-build-menu"
            ],
            "type": "ms.vss-web.action",
            "properties": {
                "title": "View associated items",
                "uri": "launch.html"
            }
        },
        {
            "id": "build.items",
            "targets": [
                "ms.vss-build-web.build-hub-group"
            ],
            "type": "ms.vss-web.hub",
            "properties": {
                "name": "Inspector",
                "order": 22,
                "uri": "items.html"
            }
        }
    ]
}